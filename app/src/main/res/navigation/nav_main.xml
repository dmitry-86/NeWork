<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/feedFragment">
    <fragment
        android:id="@+id/feedFragment"
        android:name="com.netology.nework.ui.FeedFragment"
        android:label="fragment_feed"
        tools:layout="@layout/fragment_feed" >
        <action
            android:id="@+id/action_feedFragment_to_newPostFragment"
            app:destination="@id/newPostFragment" />
        <action
            android:id="@+id/action_feedFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_feedFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_feedFragment_to_displayMapsFragment"
            app:destination="@id/displayMapsFragment" />
        <action
            android:id="@+id/action_feedFragment_to_imageFragment"
            app:destination="@id/imageFragment" />
    </fragment>
    <fragment
        android:id="@+id/newPostFragment"
        android:name="com.netology.nework.ui.NewPostFragment"
        android:label="fragment_new_post"
        tools:layout="@layout/fragment_new_post" >
        <action
            android:id="@+id/action_newPostFragment_to_feedFragment"
            app:destination="@id/feedFragment" />
        <action
            android:id="@+id/action_newPostFragment_to_createMapFragment"
            app:destination="@id/createMapFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.netology.nework.ui.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" />
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.netology.nework.ui.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" />
    <fragment
        android:id="@+id/userAccountFragment"
        android:name="com.netology.nework.ui.UserAccountFragment"
        android:label="UserAccountFragment" />
    <fragment
        android:id="@+id/displayMapsFragment"
        android:name="com.netology.nework.ui.DisplayMapsFragment"
        android:label="fragment_display_maps"
        tools:layout="@layout/fragment_display_maps" />
    <fragment
        android:id="@+id/createMapFragment"
        android:name="com.netology.nework.ui.CreateMapFragment"
        android:label="fragment_create_map"
        tools:layout="@layout/fragment_create_map" />
    <fragment
        android:id="@+id/imageFragment"
        android:name="com.netology.nework.ui.ImageFragment"
        android:label="fragment_image"
        tools:layout="@layout/fragment_image" />
    <fragment
        android:id="@+id/newEventFragment"
        android:name="com.netology.nework.ui.NewEventFragment"
        android:label="NewEventFragment" />
    <fragment
        android:id="@+id/editUserFragment"
        android:name="com.netology.nework.ui.EditUserFragment"
        android:label="fragment_edit_user"
        tools:layout="@layout/fragment_edit_user" />
    <fragment
        android:id="@+id/usersFragment"
        android:name="com.netology.nework.ui.UsersFragment"
        android:label="fragment_users"
        tools:layout="@layout/fragment_users" />
    <fragment
        android:id="@+id/uploadAvatarFragment"
        android:name="com.netology.nework.ui.UploadAvatarFragment"
        android:label="fragment_upload_avatar"
        tools:layout="@layout/fragment_upload_avatar" />
    <fragment
        android:id="@+id/newJobFragment"
        android:name="com.netology.nework.ui.NewJobFragment"
        android:label="NewJobFragment" />
</navigation>